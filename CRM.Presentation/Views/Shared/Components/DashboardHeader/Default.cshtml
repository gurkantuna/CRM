@model DashboardHeaderViewModel
<div class="row">
    <div class="col-xl-3 col-sm-6 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <div class="row">
                    <div class="col-9">
                        <div class="d-flex align-items-center align-self-start">
                            <h3 id="lblCustomerCount" class="mb-0">@Model.CustomerCount</h3>
                        </div>
                    </div>
                    <div class="col-3">
                        <div class="icon icon-box-success">
                            <span class="mdi mdi-cart"></span>

                        </div>
                    </div>
                </div>
                <h6 class="text-muted font-weight-normal">Customer Count</h6>
            </div>
        </div>
    </div>
    <div class="col-xl-3 col-sm-6 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <div class="row">
                    <div class="col-9">
                        <div class="d-flex align-items-center align-self-start">
                            <h3 id="lblUserCount" class="mb-0">@Model.UserCount</h3>
                        </div>
                    </div>
                    <div class="col-3">
                        <div class="icon icon-box-success">
                            <span class="mdi mdi-account-multiple"></span>

                        </div>
                    </div>
                </div>
                <h6 class="text-muted font-weight-normal">User Count</h6>
            </div>
        </div>
    </div>   
</div>